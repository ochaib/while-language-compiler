begin
  int n = 0;
  char c = '?';
  bool b = true;
  int[] arr = [3,1,2,-1];
  char[] foo = ['f', 'o', 'o'];
  bool[] tft = [true, false, true];

  # CHAR METHODS

  c = '2';
  b = call isdigit(c);
  println b; # true

  c = 'a';
  b = call islower(c);
  println b; # true

  c = 'A';
  b = call isupper(c);
  println b; # true

  c = 'Z';
  b = call isalpha(c);
  println b; # true

  c = '0';
  b = call isalnum(c);
  println b; # true

  c = ' ';
  b = call isspace(c);
  println b; # true

  c = '\n';
  b = call isnewline(c);
  println b; # true

  c = 'A';
  c = call tolower(c);
  println c; # 'a'

  c = 'a';
  c = call toupper(c);
  println c; # 'A'

  # MATH METHODS

  n = call min(arr);
  println n; # -1

  n = call max(arr);
  println n; # 3

  n = call abs(-42);
  println n; # 42

  n = call pow(4, 4);
  println n; # 256

  n = call rand(99);
  println n; # 16669

  # STRING METHODS

  n = call find('o', foo);
  println n; # 1

  # ARRAY METHODS

  b = call any(true, tft);
  println b; # true

  n = call indexOf(2, arr);
  println n; # 2

  println "done"
end

