{
    "name": "wacc-language-support",
    "displayName": "WACC Language Support",
    "description": "",
    "publisher": "pshah123",
    "version": "1.1.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "wacc",
            "aliases": ["WACC", "wacc"],
            "extensions": [".wacc",".wach"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "wacc",
            "scopeName": "source.wacc",
            "path": "./syntaxes/wacc.tmLanguage.json"
        }],
        "iconThemes": [{
            "id": "wacc",
            "label": "wacc",
            "path": "./icons.json"
        }]
    }
}